package console;

import main.Base;

public class ConsoleCommand extends Thread {
	
	protected String[] args;
	
	protected final Base base;
	
	protected ConsoleCommand(Base b) {
		super();
		base = b;
	}
	
	public void loadObjects() {
		args =  base.getConsole().getArgs();
	}
	
	public void run() {
		synchronized(base) {
			loadObjects();
		}
	}
	
}
