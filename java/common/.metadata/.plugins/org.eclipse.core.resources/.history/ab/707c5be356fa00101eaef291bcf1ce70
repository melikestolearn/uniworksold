package console;

import main.Base;

public class ConsoleCommand extends Thread {
	
	protected String[] objects;
	
	protected final Base base;
	
	protected ConsoleCommand(Base b) {
		super();
		base = b;
		synchronized(base) {
			loadObjects();
		}
	}
	
	protected String[] loadObjects() {
		return base.getConsole().getObjects();
	}
	
	public void run() { }
	
	
}
