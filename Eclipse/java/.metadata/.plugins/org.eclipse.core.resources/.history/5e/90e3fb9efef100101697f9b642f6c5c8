
public class TicTacToe {
	char[] fields = new char[9];
		public TicTacToe(){
			System.out.println(" | | \n_|_|_\n | | \n_|_|_\n | | \n | | ");
		}
		public void setField(char symbol,int colum,int row){
			if(row == 2)
				colum = colum + 3;
			else if(row == 3)
				colum = colum + 6;
			fields[colum] = symbol;
				
		}
}
